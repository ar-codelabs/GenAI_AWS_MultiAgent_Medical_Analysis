# AWS Configuration
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# S3 Configuration
S3_BUCKET=your-medical-bucket

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_REGION=us-east-1

# OpenSearch Configuration
OPENSEARCH_ENDPOINT=https://your-opensearch-endpoint.amazonaws.com
OPENSEARCH_INDEX=medical-multimodal-cases

# Email Notification (Optional)
ENABLE_EMAIL_ALERTS=false
DOCTOR_EMAIL=doctor@hospital.com
SES_SENDER_EMAIL=system@hospital.com

# System Configuration
LOG_LEVEL=INFO
MAX_IMAGE_SIZE_MB=10

# Local Data Path (for development)
LOCAL_DATA_PATH=/path/to/your/descriptions_total.jsonl